{
  "name": "onboard",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "",
  "private": true,
  "scripts": {
    "install:all": "npm install --from-lock-file",
    "docker": "npm run install:all && npm run dev",
    "dev": "NODE_ENV=development ENVIRONMENT=${ENVIRONMENT-local} webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "dev:mock": "MOCK=true npm run dev",
    "dev:test": "npm run test:unit:dev",
    "dev:test:mock": "MOCK=true npm run test:unit:dev",
    "dev_": "webpack --config build/webpack.dev.conf.js --watch",
    "start": "npm run dev",
    "create": "echo \"commands available \n npm run create:component \n npm run create:component \n\n \"",
    "test": "npm run test:unit && npm run e2e",
    "test:lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "test:unit": "MOCK=true jest --config jest.conf.js",
    "test:unit:dev": "jest --config jest.conf.js --watchAll",
    "test:coverage": "MOCK=true jest --config jest.conf.js --coverage",
    "lint:fix": "eslint --fix --ext .js,.vue src test/unit test/e2e/specs",
    "e2e": "node test/e2e/runner.js",
    "build": "node build/build.js",
    "_build": "node build/build.js && npm run _build:move-manifest",
    "build:stage": "ENVIRONMENT=stage npm run build",
    "build:local": "ENVIRONMENT=local npm run build",
    "build:current": "npm run build && npm run _build:move-manifest",
    "build:tag": "TAG=1 node build/build.js && npm run _build:move-manifest",
    "ci:install": "npm run install:all",
    "ci:test": "MOCK=true npm run test:unit",
    "ci:build:test": "ENVIRONMENT=test npm run build",
    "ci:build:dev": "ENVIRONMENT=dev npm run build",
    "ci:build:prod": "ENVIRONMENT=prod npm run build",
    "_build:move-manifest": "node build/move-manifest.js"
  },
